<% if(message && message.length >0){ %>
  <div class="alert alert-warning alert-dismissible" style="position: absolute;
	top: 0px;
	right: 1%;
	height: 100px;
	width: 400px;
	z-index: 99;">
    <button type="button" id="btn" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>

    </button>
    <%= message %>
  </div>
  <% } %>
    <% if(errors && errors.length >0){ %>
      <div class="alert alert-danger alert-dismissible" style="position: absolute;
  top: 0px;
  right: 1%;
  height: 100px;
  width: 400px;
  z-index: 99;">
        <button type="button" class="close" id="btn" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true">&times;</span>

        </button>

        <br>
        <%= errors %>
      </div>
      <% } %>
