                $("#userNumber").empty();
                // $("#userNumber").append('<option value="<%= adminCount %>"><%= adminCount %></option>');
                document.getElementById("section").style.display = "none";
                document.getElementById("yrlvl").style.display = "none";
                console.log(numUsers);

              } else {
                // document.getElementById("tableFaculty").style.display = "none";
                var numUsers = document.getElementById("userNumber").value = "<%= studentCount %>";
                $("#userNumber").empty();
