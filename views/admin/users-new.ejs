          <script type="text/javascript">
            for (var i = (new Date).getFullYear(); i >= 1905; i--) {
              $("#year").append('<option value="' + i + '">' + i + '</option>');
            }
            for (var i = 31; i >= 1; i--) {
              $("#day").append('<option value="' + i + '">' + i + '</option>');
            }

            function getUsers() {
              var users = document.getElementById("users").value;

              if (users === "faculty") {
                var numUsers = document.getElementById("userNumber").value = "<%= facultyCount %>";
                $("#userNumber").empty();
                // $("#userNumber").append('<option value="<%= facultyCount %>"><%= facultyCount %></option>');
                document.getElementById("section").style.display = "none";

                document.getElementById("yrlvl").style.display = "none";
                // document.getElementById("tableFaculty").style.display = "block";
                console.log(numUsers);
              } else if (users === "admin") {
